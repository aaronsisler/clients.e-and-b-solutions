{
  "name": "clients.e-and-b-solutions",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "deploy:github:beta": "export DEPLOY_ENV=beta && node ./deploy/deployment.js",
    "deploy:github:prod": "export DEPLOY_ENV=prod && node ./deploy/deployment.js",
    "prebuild": "npm run clear:next",
    "build": "next build ./src",
    "clear:next": "rm -rf ./.next",
    "clear:serverless": "rm -rf ./.serverless && rm -rf ./.serverless_nextjs",
    "dev": "npm run clear:next && next ./src",
    "domain:prod": "sls create_domain --config=./deployment/serverless.prod.yml",
    "deploy:prod": "npm run build && sls deploy --config ./deployment/serverless.prod.yml && npm run clear:serverless && npm run clear:next",
    "gen:comp": "node ./node_modules/@ebsolutions/generate-react-templates --type=COMP",
    "gen:cont": "node ./node_modules/@ebsolutions/generate-react-templates --type=CONT",
    "gen:page": "node ./node_modules/@ebsolutions/generate-react-templates --type=PAGE",
    "lint": "eslint \"./**/*.{js,jsx}\"",
    "lint1": "eslint \"./**/*.js\"",
    "lint:style": "node ./node_modules/stylelint/bin/stylelint.js \"**/*.{css,scss}\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.21.4",
    "classnames": "^2.2.6",
    "email-validator": "^2.0.3",
    "express": "^4.16.4",
    "next": "^11.1.2",
    "node-sass": "^4.12.0",
    "normalize-scss": "^7.0.1",
    "prop-types": "^15.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^4.7.2",
    "serverless-http": "^1.6.0",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@ebsolutions/generate-react-templates": "^1.6.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.1.0",
    "eslint-config-next": "12.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-standard-scss": "^2.0.0",
    "stylelint-order": "^3.1.1"
  }
}
